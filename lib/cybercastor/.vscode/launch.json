  "version": "0.2.0",
  "configurations": [
    {
      "name": "ðŸ¦« Monitor",
      "type": "python",
      "request": "launch",
      "module": "cybercastor.monitor",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "{env:CC_API_URL}",
        "{env:USERNAME}",
        "{env:PASSWORD}",
        // "--download_running",
        "--verbose"
      ]
    },
    {
      "name": "ðŸ¦« AddJob",
      "type": "python",
      "request": "launch",
      "module": "cybercastor.add_job",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "${workspaceFolder}/jobs/",
        "{env:CC_API_URL}",
        "{env:USERNAME}",
        "{env:PASSWORD}",
        "--verbose"
      ]
    },
    {
      "name": "ðŸ¦« dump_sqlite",
      "type": "python",
      "request": "launch",
      "module": "cybercastor.dump_sqlite",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      // Add NATIONAL_PROJECT, INPUTS_FOLDER, DOWNLOAD_FOLDER to your .env file
      "args": [
        "{env:DATA_ROOT}/warehouse_report",
        "{env:CC_API_URL}",
        "{env:USERNAME}",
        "{env:PASSWORD}",
        "--verbose"
      ]
    },
